{
    "name": "ukfast/laravel-api-docs",
    "description": "A library for documenting your laravel REST apis",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "George Kelly",
            "email": "george.kelly@ukfast.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "UKFast\\LaravelApiDocs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "^8.0|^9.0",
        "illuminate/routing": "^8.0|^9.0",
        "symfony/yaml": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^9.3"
    }
}
